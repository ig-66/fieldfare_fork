    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Fieldfare conf getChanges",
            "cwd": "${workspaceFolder}",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\bin\\ffconf.js",
            "args": ["getChanges"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "FieldFare Run CLI",
            "cwd": "${workspaceFolder}",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\bin\\ffrun.js",
            //"runtimeArgs": ["-r","esm"]
        }
    ]
}